<?xml version="1.0"?>
<!DOCTYPE avg SYSTEM "../../doc/avg.dtd">
<avg width="160" height="120"  
    onmousedown="mainMouseDown" onmouseup="mainMouseUp"
    onkeydown="keyDown" onkeyup="keyUp">
    <image id="mousemove1" x="0" y="0" opacity="0.8"
        href="rgb24-65x65.png" onmousemove="onMouseMove" onmouseup="onMouseUp"/>
    <image id="mouseover1" x="65" y="0"  
        href="rgb24-65x65.png" onmouseover="onMouseOver" 
        onmouseout="onMouseOut" onmousedown="onMouseDown"/>
    <div id="leftdiv" y="65">
        <image id="mousemove2" x="0" y="0" opacity="0.8"
            href="rgb24-65x65.png" onmousemove="onMouseMove" onmouseup="onMouseUp"/>
    </div>
    <div id="rightdiv" y="65">
        <image id="mouseover2" x="65" y="0" 
            href="rgb24-65x65.png" onmouseover="onMouseOver" 
            onmouseout="onMouseOut" onmousedown="onMouseDown"/>
    </div>
    <image id="tilted" x="100" y="64" width="32" height="32" angle="0.785"
        href="rgb24.png" onmouseover="onTiltedOver"/>
</avg>
