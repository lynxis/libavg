<?xml version="1.0"?>
<!DOCTYPE avg SYSTEM "../doc/avg.dtd">
<avg width="400" height="400" onmousedown="mainMouseDown();" onmouseup="mainMouseUp();">
    <image id="mousemove" x="0" y="0" z="1" opacity="0.8"
        href="rgb24.tif" onmousemove="onMouseMove();" onmouseup="onMouseUp();"/>
    <image id="mouseover" x="200" y="0" z="2" 
        href="rgb24alpha.tif" onmouseover="onMouseOver();" 
        onmouseout="onMouseOut();"/>
</avg>
